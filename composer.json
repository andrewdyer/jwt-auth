{
    "name": "andrewdyer/jwt-auth",
    "description": "A simple framework agnostic JSON Web Token authentication solution",
    "keywords": [
        "php",
        "jwt",
        "jwt-auth",
        "jwt-authentication",
        "jwt-token"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "homepage": "https://andrewdyer.rocks",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anddye\\JWTAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\JWTAuth\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "firebase/php-jwt": "^6.10"
    },
    "scripts": {
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ]
    }
}
